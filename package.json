{
  "name": "company_fy",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "rimraf": "./node_modules/rimraf/bin.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=prod nest start --watch",
    "dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.json --coverage",
    "test:watch": "jest --config ./jest.config.json --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./jest.config.json --coverage",
    "test:e2e:watch": "cross-env NODE_ENV=test jest --config ./jest.config.json --detectOpenHandles --watch"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dayjs-plugin-utc": "^0.1.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/services/*.service.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@common/(.*)": "<rootDir>/src/common/$1",
      "^@entities/(.*)": "<rootDir>/src/entities/$1",
      "^@dtos/(.*)": "<rootDir>/src/dtos/$1",
      "^@modules/(.*)": "<rootDir>/src/modules/$1",
      "^@controllers/(.*)": "<rootDir>/src/controllers/$1",
      "^@services/(.*)": "<rootDir>/src/services/$1",
      "^@config/(.*)": "<rootDir>/src/config/$1",
      "^@repositories/(.*)": "<rootDir>/src/repositories/$1",
      "^@swaggers/(.*)": "<rootDir>/src/swaggers/$1",
      "^@providers/(.*)": "<rootDir>/src/providers/$1",
      "^@utils/(.*)": "<rootDir>/src/utils/$1",
      "^@models/(.*)": "<rootDir>/src/models/$1"
    }
  },
  "_moduleAliases": {
    "@modules": "dist/modules",
    "@controllers": "dist/controllers",
    "@common": "dist/common",
    "@entities": "dist/entities",
    "@dtos": "dist/dtos",
    "@config": "dist/config",
    "@services": "dist/services",
    "@repositories": "dist/repositories",
    "@swaggers": "dist/swaggers",
    "@providers": "dist/providers",
    "@utils": "dist/utils",
    "@models": "dist/models"
  }
}
